<Group hide-input="false" labelreference="L17" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">nbs := proc(s)
option remember;
local output, tmpc, tmps, tmph, state, i, j;
    output := NULL;
    state := string2state(s);


    # room to room
    for i from 1 to 4 do
        for j from 1 to 4 do
            if i = j then next; end if;
            try:
                tmpc,tmps := moveR2R( state, i, j);
                output := output, [tmpc, state2string(tmps),0];
            catch:
            end try;
         end do;
     end do;

  if nops([output]) &gt; 0 then return [output] end if;


    # hall to room
    for i in [1,2,4,6,8,10,11] do
        for j from 1 to 4 do
            try:
                tmpc,tmps := moveH2R( state, i, j);
                output := output, [tmpc, state2string(tmps),0];
            catch:
            end try;
         end do;
     end do;

    if nops([output]) &gt; 0 then return [output] end if;


    # room to hall
    for i in [1,2,4,6,8,10,11] do
        for j from 1 to 4 do
            try:
                tmpc,tmps := moveR2H( state, j, i);
                tmph := ilog2(tmpc)*10;            
                output := output, [tmpc, state2string(tmps), tmph];
            catch:
            end try;
         end do;
     end do;

     return [output];
end proc:</Text-field>
</Input>
</Group>
