<Group labelreference="L14" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Dijkstra := proc(start, theend, Bound)
local pq, costsofar, curr, nb, newcost, pt; 
uses priqueue;
#option trace;

costsofar := table(sparse=Bound):


initialize(pq):
insert([0,start], pq):
costsofar[start] := 0:

while pq[0] &lt;&gt; 0 do
   curr := extract(pq)[2];
   if curr = theend then
       return costsofar[theend];
   end if;

   nb := nbs(curr);
   for pt in nb do
       newcost := costsofar[curr] + pt[1];
       if newcost &lt; costsofar[pt[2]] then
           costsofar[pt[2]] := newcost;
           insert([-newcost#+pt[1]*99/100-pt[3]
, pt[2]], pq);
       end if;
   end do;
end do:

return costsofar[theend];

end proc:</Text-field>
</Input>
</Group>
