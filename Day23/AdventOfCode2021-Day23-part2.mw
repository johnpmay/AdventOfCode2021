<Group hide-input="false" labelreference="L5" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">moveR2H := proc(state, room, pos, {r2r::truefalse:=false})
  local newstate, newroom, actor, steps, start;

  if pos in [3,5,7,9] and not r2r then
     error &quot;illegal position in the hallway&quot;, pos;
  elif state[1][pos] &lt;&gt; &quot;.&quot; then
     error &quot;position occupied&quot;, pos, state[1][pos];
  end if;
start := roompos[room];

  if state[2][room] = [&quot;.&quot;,&quot;.&quot;,&quot;.&quot;,&quot;.&quot;] then error &quot;room empty&quot;, room;
  elif state[2][room][1] &lt;&gt; &quot;.&quot; then
     actor := state[2][room][1];
     steps := 1;
     newroom := [&quot;.&quot;,state[2][room][2..-1][]];
  elif state[2][room][2] &lt;&gt; &quot;.&quot; then
     actor := state[2][room][2];
     steps := 2;
     newroom := [&quot;.&quot;,&quot;.&quot;, state[2][room][3..-1][]];
  elif state[2][room][3] &lt;&gt; &quot;.&quot; then
     actor := state[2][room][3];
     steps := 3;
     newroom := [&quot;.&quot;,&quot;.&quot;,&quot;.&quot;, state[2][room][4]];
  else
     actor := state[2][room][4];
     steps := 4;
     newroom := [&quot;.&quot;, &quot;.&quot;, &quot;.&quot;,&quot;.&quot;];
  end if;

  local path := state[1][`..`(min,max)(pos,start)];  
  if nops(path)&gt;2 and {path[2..-2][]} &lt;&gt; {&quot;.&quot;} then
     error &quot;path blocked&quot;;
  end if;

  steps += abs(pos - start);

  newstate := [subsop(pos=actor, state[1]), subsop(room=newroom, state[2])];

  return steps*movecost[actor], newstate;

end proc:

moveH2R := proc(state, pos, room::posint:=validroom[state[1][pos]], {r2r::truefalse:=false})
  local newstate, newroom, actor, steps, start;
#option trace;
  if pos in [3,5,7,9] and not r2r then
     error &quot;illegal position in the hallway&quot;, pos;
  end if;
  actor := state[1][pos];
  if validroom[actor] &lt;&gt; room then
     error &quot;illegal room for actor&quot;, room, actor;
  end if;
  if actor = &quot;.&quot; then
     error &quot;position empty&quot;, pos;
  elif state[2][room][1] &lt;&gt; &quot;.&quot; then
     error &quot;room full&quot;, room;
  elif state[2][room][4] = &quot;.&quot; then
     steps := 4;
     newroom := [&quot;.&quot;,&quot;.&quot;,&quot;.&quot;,actor];
  elif state[2][room][3] = &quot;.&quot; then
     steps := 3;
     newroom := [&quot;.&quot;,&quot;.&quot;,actor,state[2][room][4]];
  elif state[2][room][2] = &quot;.&quot; then
     steps := 2;
     newroom := [&quot;.&quot;,actor, state[2][room][3..-1][]];
  else
     steps := 1;
     newroom := [actor, state[2][room][2..-1][]];
  end if;

  if {state[2][room][]} minus {&quot;.&quot;, actor} &lt;&gt; {} then
     error &quot;cannot enter a room until empty of wrong types&quot;, actor, state[2][room]
;
  end if;
  local path := state[1][`..`(min,max)(pos,roompos[room])];  
  if nops(path)&gt;2 and {path[2..-2][]} &lt;&gt; {&quot;.&quot;} then
     error &quot;path blocked&quot;;
  end if;

  steps += abs(pos - roompos[room]);

  newstate := [subsop(pos=&quot;.&quot;, state[1]), subsop(room=newroom, state[2])];

  return steps*movecost[actor], newstate;

end proc:
moveR2R := proc(state, room1, room2)
local c1, c2, ns;
   (c1, ns) := moveR2H(state, room1, roompos[room2], 'r2r');
   (c2, ns) := moveH2R(ns, roompos[room2], room2, 'r2r');
   return (c1+c2), ns;
end proc:
</Text-field>
</Input>
</Group>
